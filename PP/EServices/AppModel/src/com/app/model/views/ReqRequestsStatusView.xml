  <SQLQuery>
    <![CDATA[SELECT ReqRequests.REQ_ID, 
       ReqRequests.REQ_NAME,
(select ms .name from moj_services ms where ms.language = 'en' and ms.request_service_name = ReqRequests.req_name and ms.translated_service_id = ReqRequests.service_id)"EngName",
(select ms .name from moj_services ms where ms.language = 'ar' and ms.request_service_name = ReqRequests.req_name and ms.id = ReqRequests.service_id)"ArabicName", 
(select msj.name from moj_services msj where language = 'ar' and msj.id = (select moj.translated_service_id from moj_services moj where moj.name = ReqRequests.REQ_NAME)) reqNameAr,
       ReqRequests.REQ_CLASS, 
       ReqRequests.STATUS, 
       ReqRequests.CREATED_DATE, 
       ReqRequests.MODIFIED_DATE, 
       ReqRequests.PUBLIC_USER_ID, 
       ReqRequests.SERVICE_ID, 
       ReqRequests.PROSEC_ID, 
       ReqRequests.REF_NO, 
       ReqRequests.STATUS_DETAILS, 
       ReqRequests.CASE_INFO_ID,
(select epc.prosecution_eng_name from ENV_PROSECUTIONS_CITY epc where epc.prosecution_id =  ReqRequests.PROSEC_ID) "Pros_en_name",
(select epc.prosecution_name from ENV_PROSECUTIONS_CITY epc where epc.prosecution_id =  ReqRequests.PROSEC_ID) "Pros_ar_name"
FROM REQ_REQUESTS ReqRequests
WHERE public_user_id = :puid
group by ReqRequests.req_id,ReqRequests.req_name,
ReqRequests.REQ_CLASS, 
       ReqRequests.STATUS, 
       ReqRequests.CREATED_DATE, 
       ReqRequests.MODIFIED_DATE, 
       ReqRequests.PUBLIC_USER_ID, 
       ReqRequests.SERVICE_ID, 
       ReqRequests.PROSEC_ID, 
       ReqRequests.REF_NO, 
       ReqRequests.STATUS_DETAILS, 
       ReqRequests.CASE_INFO_ID order by req_id desc]]>
  </SQLQuery>
