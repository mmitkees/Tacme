<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ChangePwrd"
  Version="11.1.1.66.73"
  InheritPersonalization="true"
  SelectList="PuPublicuser.ID, 
       PuPublicuser.PASSWORD, 
       PuPublicuser.EMAIL"
  FromList="PU_PUBLICUSER PuPublicuser"
  Where="id = :b_id and password = :b_pwrd"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.app.model.views.ChangePwrd_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="b_id"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <Variable
    Name="b_pwrd"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="PuPublicuser"
    Entity="com.app.model.entities.PuPublicuser"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="PuPublicuser"
    AliasName="ID"/>
  <ViewAttribute
    Name="Password"
    PrecisionRule="true"
    EntityAttrName="Password"
    EntityUsage="PuPublicuser"
    AliasName="PASSWORD"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.app.model.AppModelBundle"/>
  </ResourceBundle>
  <ViewAttribute
    Name="Email"
    IsUnique="true"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Email"
    EntityUsage="PuPublicuser"
    AliasName="EMAIL"/>
</ViewObject>
