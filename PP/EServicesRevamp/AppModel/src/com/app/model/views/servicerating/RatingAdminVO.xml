<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="RatingAdminVO"
  Version="11.1.1.66.73"
  InheritPersonalization="merge"
  OrderBy="Rating_key"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.app.model.views.servicerating.RatingAdminVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="key"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT RatingRecordsEO.RATING_KEY,RatingValues.RATING_VALUE_EN,RatingValues.RATING_VALUE_AR,
       count(RatingRecordsEO.RATING_KEY) AS rating_count, (select count(*)from RATING_RECORDS) As total_count
FROM RATING_RECORDS RatingRecordsEO ,RATING_VALUES RatingValues
where RatingValues.rating_key=RatingRecordsEO.rating_key and RatingValues.rating_key=nvl(:key ,RatingValues.rating_key) 
group by RatingRecordsEO.RATING_KEY,RatingValues.RATING_VALUE_EN,RatingValues.RATING_VALUE_AR]]>
  </SQLQuery>
  <ViewAttribute
