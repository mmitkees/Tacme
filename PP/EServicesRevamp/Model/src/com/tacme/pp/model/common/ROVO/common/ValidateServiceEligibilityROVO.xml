<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ValidateServiceEligibilityROVO"
  Version="11.1.1.66.73"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.tacme.pp.model.service.common.REVO.ValidateServiceEligibilityROVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_cno"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <Variable
    Name="p_cyear"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <Variable
    Name="p_prosid"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <Variable
    Name="p_uemid"
    Kind="where"
    Type="java.lang.String"/>
  <ViewAccessor
    Name="CaseYearLOV1"
    ViewObjectName="com.tacme.pp.model.common.LOV.CaseYearLOV"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="CaseProscutionLOV_EN1"
    ViewObjectName="com.tacme.pp.model.common.LOV.CaseProscutionLOV_EN"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="CaseProscutionLOV_AR1"
    ViewObjectName="com.tacme.pp.model.common.LOV.CaseProscutionLOV_AR"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="CaseRelationLOV_AR1"
    ViewObjectName="com.tacme.pp.model.common.LOV.CaseRelationLOV_AR"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="CaseRelationLOV_EN1"
    ViewObjectName="com.tacme.pp.model.common.LOV.CaseRelationLOV_EN"
    RowLevelBinds="true"/>
  <ListBinding
    Name="LOV_Caseyear"
    ListVOName="CaseYearLOV1"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="Caseyear"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="DateValue"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <ListBinding
    Name="LOV_ProsecutionId_en"
    ListVOName="CaseProscutionLOV_EN1"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="ProsecutionId"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="ProsecutionId"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="ProsecutionName"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <ListBinding
    Name="LOV_ProsecutionId_ar"
    ListVOName="CaseProscutionLOV_AR1"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="ProsecutionId"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="ProsecutionId"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="ProsecutionName"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <SQLQuery>
    <![CDATA[SELECT caseid, 
       caseno, 
       caseyear, 
       partyid, 
       partytype, 
       idcardno, 
       prosecution_id, 
       event_status_lkp, 
       event_status_desc 
FROM   prc_party_req_view 
WHERE  caseno = :p_cno 
       AND caseyear = :p_cyear 
       AND prosecution_id = :p_prosid 
       AND idcardno = :p_uemid]]>
  </SQLQuery>
  <ViewAttribute
    Name="Caseid"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="CASEID"
    Expression="CASEID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Caseno"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="CASENO"
    Expression="CASENO"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Caseyear"
    LOVName="LOV_Caseyear"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="CASEYEAR"
    Expression="CASEYEAR"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Partyid"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="PARTYID"
    Expression="PARTYID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Partytype"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="PARTYTYPE"
    Expression="PARTYTYPE"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Idcardno"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="IDCARDNO"
    Expression="IDCARDNO"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ProsecutionId"
    LOVName="LOV_ProsecutionId_en"
    LOVFinder="ProsecutionIdLOVSelector"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="PROSECUTION_ID"
    Expression="PROSECUTION_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="EventStatusLkp"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="EVENT_STATUS_LKP"
    Expression="EVENT_STATUS_LKP"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EventStatusDesc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EVENT_STATUS_DESC"
    Expression="EVENT_STATUS_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="locale"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VIEW_ATTR"
    SQLType="VARCHAR">
    <TransientExpression><![CDATA[adf.context.expressionEvaluator.evaluate('#{pageFlowScope.locale}')]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="ProsecutionIdLOVSelector"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VIEW_ATTR"
    SQLType="VARCHAR">
    <TransientExpression><![CDATA[if (locale != null && locale == 'ar'){ return 'LOV_ProsecutionId_ar'; }else{ return 'LOV_ProsecutionId_en'; }]]></TransientExpression>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.tacme.pp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
