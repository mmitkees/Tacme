<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CaseYearLOV"
  Version="11.1.1.66.73"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.tacme.pp.model.common.LOV.CaseYearLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[WITH dat AS
  (SELECT to_date('01-JAN-1990','DD-MON-YYYY') from_date,
    (SELECT sysdate FROM dual
    ) end_date
  FROM dual
  )
SELECT TO_CHAR(from_date,'YYYY')+level-1 Date_Value

FROM dat
  CONNECT BY level <= TO_CHAR(end_date,'YYYY')-TO_CHAR(from_date,'YYYY')+1
ORDER BY Date_Value DESC]]>
  </SQLQuery>
  <ViewAttribute
    Name="DateValue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="DATE_VALUE"
    Expression="DATE_VALUE"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.tacme.pp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
